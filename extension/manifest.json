{
  "name": "Github tools",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Extends github projects - hides clutter and adds filtering",
  "content_scripts": [
    {
      "matches": [
        "https://github.schibsted.io/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "contentScript.js", "filtering.js"
      ]
    }
  ]
}
